    "test:a11y": "playwright test --grep @a11y",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "verify:full": "npm run verify && npm run test:e2e",
    "analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
